{
  "name": "style_resolver",
  "version": "0.0.1",
  "description": "style resolver solve import to disk location file",
  "main": "dist/index.js",
  "types": "binding.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "napi": {
    "name": "style_resolver",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "version": "napi version",
    "build:napi": "napi build --platform --release --js false --dts binding.d.ts",
    "test": "npx ts-node tslib/index.ts --content '{\"content\":\".a{width:10px;}\"}'",
    "test:resolve": "npx ts-node tslib/test.ts",
    "cp": "rm -rf ../style/js-lib/** && cp -rf ./dist ../style/js-lib",
    "build": "npm run build:napi && npx ts-node tslib/build.ts && npm run cp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@napi-rs/cli": "2.9.0",
    "@types/enhanced-resolve": "3.0.7",
    "@types/less": "3.0.3",
    "@types/node": "17.0.32",
    "enhanced-resolve": "5.9.3",
    "esbuild": "0.14.39",
    "less": "4.1.2",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  }
}
